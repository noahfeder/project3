<div class="container">
  <div class="header">
    <div class="left">
      <div class="soundcloud"></div>
    </div>
    <div class="center">
      <div class="welcome">
        <h1></h1>
        <input id="edit_name">
      </div>
    </div>
    <div class="right">
      <div class="weather"></div>
